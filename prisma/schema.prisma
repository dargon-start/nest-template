generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ErrorList {
  id        Int     @id @default(autoincrement())
  errorName String  @map("error_name")
  errorType String  @map("error_type")
  createTime DateTime? @default(now()) @map("create_time")

  @@map("error_list")
}
